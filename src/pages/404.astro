<style>
  /* 简单向左箭头 */
  .simple-arrow-left {
    width: 40px;
    height: 40px;
    position: relative;
    transition: transform 0.2s ease;
  }
  /* 箭头头部 */
  .simple-arrow-left::before {
    content: "";
    position: absolute;
    width: 0;
    height: 0;
    border-top: 12px solid transparent;
    border-bottom: 12px solid transparent;
    border-right: 18px solid #374151;
    top: 8px;
    left: 4px;
  }
  /* 箭头杆 */
  .simple-arrow-left::after {
    content: "";
    position: absolute;
    width: 20px;
    height: 5px;
    background: #374151;
    top: 17px;
    left: 16px;
  }
  /* 简单重新加载图标 */
  .simple-reload {
    width: 40px;
    height: 40px;
    position: relative;
    transition: transform 0.2s ease;
  }
  .simple-reload::before {
    content: "";
    position: absolute;
    width: 26px;
    height: 26px;
    border: 5px solid #374151;
    border-radius: 50%;
    border-top-color: transparent;
    top: 4px;
    left: 4px;
  }
  .simple-reload::after {
    content: "";
    position: absolute;
    width: 0;
    height: 0;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-bottom: 10px solid #374151;
    top: 2px;
    left: 21px;
  }
  /* 悬停效果 */
  .group:hover .simple-arrow-left,
  .group:hover .simple-reload {
    transform: scale(1.1);
  }
  .group:hover .simple-arrow-left::before {
    border-right-color: #1f2937;
  }
  .group:hover .simple-arrow-left::after {
    background: #1f2937;
  }
  .group:hover .simple-reload::before {
    border-color: #1f2937;
    border-top-color: transparent;
  }
  .group:hover .simple-reload::after {
    border-bottom-color: #1f2937;
  }
</style>
